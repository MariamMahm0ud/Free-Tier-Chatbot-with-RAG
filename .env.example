# Website to crawl
ROOT_URL=https://quotes.toscrape.com/

# Maximum number of pages to scrape
MAX_PAGES=200

# Rate limit in seconds between requests
RATE=1.0

# GPT4All model path (optional, leave empty to disable LLM)
# Example: GPT4ALL_MODEL=C:/path/to/ggml-model.bin
# Download models from: https://gpt4all.io/index.html
GPT4ALL_MODEL=

# RAG API URL (for frontend)
RAG_API_URL=http://localhost:8000
